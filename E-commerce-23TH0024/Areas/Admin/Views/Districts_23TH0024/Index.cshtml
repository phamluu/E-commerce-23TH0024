@model IEnumerable<E_commerce_23TH0024.Models.Location.District>

@{
    ViewBag.Title = "Danh sách quận/ huyện";
}

<h4 class="title">@ViewBag.Title</h4>

<p>
    @Html.ActionLink("Thêm mới", "Create", "", new { @class = "btn btn-add" })
    @Html.ActionLink("ImportExcel", "ImportExcel", "", new { @class = "btn btn-importexcel" })
    @Html.ActionLink("ImportExcel", "ImportExcel", "", new { @class = "btn btn-importexcel" })
</p>
<table id="grid">
    <thead>
        <tr>
            <th> @Html.DisplayName("Id")</th>
            <th>
                @Html.DisplayName("Quận huyện")
            </th>
            <th>
                @Html.DisplayName("Thành phố")
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.DistrictName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.City.CityName)
                </td>
                <td>
                   @Html.Partial("_TableActions", item.Id)
            </tr>
        }

    </tbody>
    
</table>
