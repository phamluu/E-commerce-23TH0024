@model IEnumerable<E_commerce_23TH0024.Models.Location.City>

@{
    ViewBag.Title = "Danh sách tỉnh/ thành phố";
}

<h4 class="title">@ViewBag.Title</h4>

<p>
    @Html.ActionLink("Thêm mới", "Create", "", new { @class = "btn btn-add" })
    @Html.ActionLink("ImportExcel", "ImportExcel", "", new { @class = "btn btn-importexcel" })
    @Html.ActionLink("ExportExcel", "ExportExcel", "", new { @class = "btn btn-exportexcel" })
</p>
<table id="grid">
    <thead>
        <tr>
            <th>
                @Html.DisplayName("Id")
            </th>
            <th>
                @Html.DisplayName("Tên thành phố")
            </th>
            <th>Chức năng</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CityName)
                </td>
                <td>
                    @Html.Partial("_TableActions", item.Id)
                </td>
            </tr>
        }
            </tbody>
    

</table>
