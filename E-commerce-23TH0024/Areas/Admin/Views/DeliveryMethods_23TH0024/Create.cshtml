@model E_commerce_23TH0024.Models.DeliveryMethod

@{
    ViewBag.Title = "Phương thức giao hàng";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4 class="title">Phương thức giao hàng</h4>
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        
        @Html.Partial("_Fields")

        <div class="form-group row mt-3">
            <div class="col-md-2"></div>
            <div class="col-md-10">
                <input type="submit" value="Lưu" class="btn btn-default btn-save" />
                @Html.ActionLink("Quay lại", "Index", "", new {@class="btn btn-list"})
            </div>
        </div>
    </div>
}
